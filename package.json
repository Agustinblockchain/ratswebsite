{
  "name": "npha-site",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "start": "node dist/index.js",
    "scss": "sass ./static/stylesheets/style.scss ./static/stylesheets/style.css --style=\"compressed\" --watch",
    "watch": "tsc-watch --noClear -p tsconfig.json --onSuccess 'node dist/index.js'",
    "prettier": "prettier --write package.json tsconfig.json src/**/*.ts views/*.pug",
    "migrate": "prisma migrate dev --name init"
  },
  "engines": {
    "node": ">=18"
  },
  "prisma": {
    "schema": "database/schema.prisma"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "@prisma/client": "^4.9.0",
    "@sentry/node": "^7.31.1",
    "@sentry/tracing": "^7.31.1",
    "@types/koa": "^2.13.4",
    "@types/koa-compress": "^4.0.3",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static": "^4.0.2",
    "@types/minio": "^7.0.9",
    "@types/node": "^18.11.18",
    "@types/pug": "^2.0.5",
    "axios": "^1.2.3",
    "bech32": "^2.0.0",
    "dotenv": "^16.0.3",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.13.1",
    "koa-body": "^6.0.1",
    "koa-compress": "^5.1.0",
    "koa-mount": "^4.0.0",
    "koa-router": "^12.0.0",
    "koa-static": "^5.0.0",
    "prisma": "^4.9.0",
    "pug": "^3.0.2",
    "sass": "^1.54.0",
    "sharp": "^0.31.3",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@prettier/plugin-pug": "^2.3.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "tsc-watch": "^6.0.0"
  }
}
